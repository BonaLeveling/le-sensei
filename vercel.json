{
    "version": 2,
    "builds": [
      {
        "src": "lesensei/package.json",
        "use": "@vercel/static-dist",
        "config": { "distDir": "dist" }
      },
      {
        "src": "lesenseiBack/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/lesenseiBack/(.*)", "dest": "/lesenseiBack/$1.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  